<%- include('partials/header'); -%>
<div class="box">
    <h1>Home</h1>
    <p><%= hometext %></p>
</div>
<div class="box-down">
    <% posts.forEach(function(post){ %>
        <h1> <%= post.title %> </h1>
        <p> <%= post.desc.substring(0,50)%> </p>
        <a class="read" href="/post/<%=post.title%>">Read More...</a>
        <hr>
    <% }) %>
</div>
<%- include('partials/footer'); -%>
